{
  "name": "aliyundrive-rename",
  "type": "module",
  "version": "0.5.4",
  "private": true,
  "packageManager": "pnpm@8.15.0",
  "description": "Batch rename files of aliyundrive.",
  "author": "a1mersnow",
  "repository": "https://github.com/a1mersnow/aliyundrive-rename",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "release": "node release.mjs"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.2",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@iconify-json/carbon": "^1.1.28",
    "@iconify-json/ion": "^1.1.15",
    "@types/node": "^20.11.8",
    "@unocss/eslint-config": "^0.58.4",
    "@unocss/eslint-plugin": "^0.58.4",
    "@unocss/reset": "^0.58.4",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^8.56.0",
    "eslint-define-config": "^2.1.0",
    "execa": "^8.0.1",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "pnpm": "^8.15.0",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.1",
    "typescript": "^5.3.3",
    "unocss": "^0.58.4",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^4.5.2",
    "vite-plugin-monkey": "^3.5.1",
    "vitest": "^1.2.2",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
