{
  "name": "aliyundrive-rename",
  "type": "module",
  "version": "1.0.3",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "description": "Batch rename files of aliyundrive.",
  "author": "a1mersnow",
  "repository": "https://github.com/a1mersnow/aliyundrive-rename",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks",
    "release": "node release.mjs"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.3",
    "@iconify-json/carbon": "^1.1.36",
    "@iconify-json/ion": "^1.1.18",
    "@types/node": "^20.14.10",
    "@unocss/eslint-config": "^0.61.3",
    "@unocss/eslint-plugin": "^0.61.3",
    "@unocss/preset-rem-to-px": "^0.61.3",
    "@unocss/reset": "^0.61.3",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/test-utils": "^2.4.6",
    "animated-unocss": "^0.0.6",
    "eslint": "^9.6.0",
    "eslint-define-config": "^2.1.0",
    "execa": "^9.3.0",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "pnpm": "^9.5.0",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.14.2",
    "typescript": "^5.5.3",
    "unocss": "^0.61.3",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.2",
    "vite": "^5.3.3",
    "vite-plugin-monkey": "^4.0.4",
    "vitest": "^2.0.1",
    "vue-tsc": "^2.0.26"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
